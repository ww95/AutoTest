[
  {
    "description": "重定向到百度",
    "request": {
      "uri": "/redirect"
    },
    "redirectTo":"http://www.baidu.com"
  },
  {
    "description": "重定向到自己的网页",
    "request": {
      "uri": "/redirect/toPath"
    },
    "redirectTo": "/redirect/newRedirect"
  },
  {
    "description":"这是被重定向的请求",
    "request": {
      "uri": "/redirect/newRedirect"
    },
    "response": {
      "text": "重定向成功了",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  }
]